    {
        "id": "cbddeae7941e27fa",
        "type": "function",
        "z": "f6f2187d.f17ca8",
        "name": "hourly avg temp",
        "func": "// store temps in node context\nlet data = context.get(\"data\") || [];\n\ndata.push(msg.payload.temperature);\n\n// keep only last 6 readings (10 min Ã— 6 = 1 hour)\nif (data.length > 6) {\n    data.shift();\n}\n\n// calculate average\nlet sum = data.reduce((a, b) => a + b, 0);\nlet avg = sum / data.length;\n\n// save back\ncontext.set(\"data\", data);\nconst time = new Date();\n\n// output average\nmsg.payload = [avg, time];\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
